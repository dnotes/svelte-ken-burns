{
	"name": "svelte-ken-burns",
	"homepage": "https://svelte-ken-burns.pages.dev",
	"version": "0.0.2",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-package && rm package/*cms*.* && sed -i '.bak' '/cms\\./d;s/index\\.js\",/index.js\"/' package/package.json && rm package/package.json.bak",
