	},
	"dependencies": {
		"svelte": "^3.54.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/adapter-static": "^1.0.6",
		"@sveltejs/kit": "^1.0.0",
		"@sveltejs/package": "^1.0.2",
		"svelte-check": "^3.0.1",
		"sveltecms": "^0.0.38",
		"tslib": "^2.4.1",
		"typescript": "^4.9.3",
		"vite": "^4.0.0"
	},
	"type": "module"
}
